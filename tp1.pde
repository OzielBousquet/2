void setup ()
{
  size (600, 600);
  background (0);
  colorMode (HSB, 360, 100, 100);
}

void draw ()
{   //Trialgulo de colo

  //Ellipse Rojo
  fill (0, 90, 90);
  ellipse (300, 50, 75, 75);
  
  fill (30, 90, 90);
  ellipse (375, 150, 75, 75);
  
  fill (60, 90, 90);
  ellipse (440, 275, 75, 75);
  
  fill (90, 90, 90);
  ellipse (500, 400, 75, 75);
  
